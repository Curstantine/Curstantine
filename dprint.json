{
	"useTabs": true,
	"lineWidth": 120,
	"indentWidth": 4,
	"newLineKind": "lf",
	"excludes": ["**/node_modules", "**/*-lock.json", "**/dist/"],
	"typescript": {
		"quoteProps": "asNeeded"
	},
	"markup": {
		"astro.styleIndent": true,
		"astro.scriptIndent": true
	},
	"plugins": [
		"https://plugins.dprint.dev/typescript-0.95.12.wasm",
		"https://plugins.dprint.dev/json-0.21.0.wasm",
		"https://plugins.dprint.dev/markdown-0.20.0.wasm",
		"https://plugins.dprint.dev/g-plane/markup_fmt-v0.24.1.wasm",
		"https://plugins.dprint.dev/g-plane/malva-v0.15.0.wasm"
	]
}
